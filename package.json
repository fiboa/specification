{
  "name": "fiboa-specification",
  "version": "0.1.0",
  "scripts": {
    "test": "npm run check-docs && npm run check-examples",
    "check-examples": "npm run create-json-schema && ajv --spec=draft2020 -s geojson/schema/schema.json -d \"geojson/examples/*.json\" -r \"geojson/schema/!(schema).json\" -c ajv-formats",
    "check-docs": "remark . -f -r .github/.remarkrc",
    "create-json-schema": "node ./geojson/scripts/create_schema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiboa/specification.git"
  },
  "author": "fiboa contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fiboa/specification/issues"
  },
  "homepage": "https://github.com/fiboa/specification",
  "devDependencies": {
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.1",
    "remark-cli": "^12.0.0",
    "remark-gfm": "^4.0.0",
    "remark-lint": "^9.1.2",
    "remark-lint-no-html": "^3.1.2",
    "remark-preset-lint-consistent": "^5.1.2",
    "remark-preset-lint-markdown-style-guide": "^5.1.3",
    "remark-preset-lint-recommended": "^6.1.3",
    "remark-validate-links": "^13.0.0",
    "yaml": "^2.3.4"
  }
}
